{
  "name": "ZaloSDKFramework",
  "version": "2.2.0616",
  "summary": "Zalo SDK Framework",
  "description": "Zalo software development kit for iOS\nDocument: http://developers.zalo.me/docs/sdk/ios-sdk-9",
  "homepage": "http://developers.zalo.me/docs/sdk/ios-sdk-9",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Liem Vo": "liemvouy@gmail.com"
  },
  "source": {
    "git": "https://github.com/liemvu/ZaloSDKFramework.git",
    "tag": "2.2.0616"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "ZaloSDKFramework/Classes/**/*",
  "frameworks": [
    "UIKit",
    "MapKit",
    "ZaloSDKCoreKit",
    "ZaloSDK"
  ],
  "vendored_frameworks": [
    "ZaloSDKFramework/Frameworks/ZaloSDKCoreKit.framework",
    "ZaloSDKFramework/Frameworks/ZaloSDK.framework"
  ],
  "prefix_header_contents": "#import <ZaloSDK/ZaloSDK.h>",
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -framework ZaloSDK -framework ZaloSDKCoreKit",
    "FRAMEWORK_SEARCH_PATHS": "${PODS_ROOT}/ZaloSDKFramework/ZaloSDKFramework/Frameworks/**"
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  }
}
